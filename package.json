{
  "name": "learn-improv-online-site",
  "description": "Site for LeanImprovOnline.com",
  "version": "1.0.0-alpha",
  "author": "Luke Dodge <luke@learnimprovonline.com>",
  "homepage": "https://www.learnimprovonline.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/lrdodge/learnimprovonline.github.io"
  },
  "dependencies": {
    "gatsby": "^1.9.232",
    "gatsby-link": "^1.6.24",
    "gatsby-plugin-manifest": "^1.0.12",
    "gatsby-plugin-offline": "^1.0.12",
    "gatsby-plugin-react-helmet": "^1.0.8",
    "gatsby-plugin-sitemap": "^1.2.9"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "test": "jest",
    "test:watch": "jest --watchAll --verbose",
    "snapshots": "jest --updateSnapshot"
  },
  "jest": {
    "testRegex": ".+test\\.js$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.cache"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "./src/utils/enzyme.js",
    "collectCoverage": true
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-to-json": "^3.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "prettier": "^1.11.1",
    "react-test-renderer": "^15.0.0"
  }
}
